Dim BallRollVolume : BallRollVolume = 0.5   'Level of ball rolling volume. Value between 0 and 1
Dim RampRollVolume : RampRollVolume = 0.5   'Level of ramp rolling volume. Value between 0 and 1

'Dim Cabinetmode  : Cabinetmode = 0     '0 - Siderails On, 1 - Siderails Off
Dim VRRoomChoice : VRRoomChoice = 2     '1 - Minimal Room,  2 - Mega Room,  3 - Ultra Minimal

Dim LiveCabViewVRMode : LiveCabViewVRMode = 0

' Called when options are tweaked by the player.
' - 0: game has started, good time to load options and adjust accordingly
' - 1: an option has changed
' - 2: options have been reseted
' - 3: player closed the tweak UI, good time to update staticly prerendered parts
' Table1.Option arguments are:
' - option name, minimum value, maximum value, step between valid values, default value, unit (0=None, 1=Percent), an optional arry of literal strings
Dim dspTriggered : dspTriggered = False
Sub Table1_OptionEvent(ByVal eventId)
    If eventId = 1 And Not dspTriggered Then dspTriggered = True : DisableStaticPreRendering = True : End If

  Dim BP

  RefractOpt = Table1.Option("Refraction Setting", 0, 2, 1, 2, 0, Array("No Refraction (best performance)", "Sharp Refractions (improved performance)", "Rough Refractions (best visuals)"))
  SetRefractionProbes RefractOpt

  'Difficulty
  OutPostMod = Table1.Option("Outlane Difficulty", 0, 2, 1, 1, 0, Array("Easy", "Medium", "Hard"))

  If OutPostMod = 0 Then ' Easy
    rout_easy.collidable = true
    rout_medium.collidable = false
    rout_hard.collidable = false
    lout_easy.collidable = true
    lout_medium.collidable = false
    lout_hard.collidable = false

    For Each BP in BP_leftpost_peg: BP.x = lout_easy.x: BP.y = lout_easy.y: Next
    For Each BP in BP_leftpost_rubber: BP.x = lout_easy.x: BP.y = lout_easy.y: Next
    For Each BP in BP_leftpost_screw: BP.x = lout_easy.x: BP.y = lout_easy.y: Next
    For Each BP in BP_leftpost_washer: BP.x = lout_easy.x: BP.y = lout_easy.y: Next

    For Each BP in BP_rightpost_peg: BP.x = rout_easy.x: BP.y = rout_easy.y: Next
    For Each BP in BP_rightpost_rubber: BP.x = rout_easy.x: BP.y = rout_easy.y: Next
    For Each BP in BP_rightpost_screw: BP.x = rout_easy.x: BP.y = rout_easy.y: Next
    For Each BP in BP_rightpost_washer: BP.x = rout_easy.x: BP.y = rout_easy.y: Next

  ElseIf OutPostMod = 1 Then ' Medium
    rout_easy.collidable = false
    rout_medium.collidable = true
