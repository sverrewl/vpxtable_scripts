Sub UpDownTimer_Timer()
  Dim timestep

  If BendTime <> 0 Then
    timestep = gametime - BendTime
    If RexBendRot < trforwardangle + 1 or RexBendRot > -1 Then
      RexBendRot = RexBendRot + RexBendDir*timestep/100
    Else
      RexBendRot = RexBendRot + RexBendDir*timestep/10
