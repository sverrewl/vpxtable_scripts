Sub Table1_Init
  vpmInit Me
  With Controller
    .GameName = cGameName
    If Err Then MsgBox "Can't start Game" & cGameName & vbNewLine & Err.Description:Exit Sub
    .SplashInfoLine = "Jurassic Park (Data East 1993) VPW"
    .Games(cGameName).Settings.Value("rol") = 0
    .HandleMechanics = 0
    .HandleKeyboard = 0
    .ShowDMDOnly = 1
    .ShowFrame = 0
    .ShowTitle = 0
    .Hidden = 0
    On Error Resume Next
    .Run GetPlayerHWnd
    If Err Then MsgBox Err.Description
    On Error Goto 0
  End With

  '************  Main Timer init  ********************

  PinMAMETimer.Interval = PinMAMEInterval
  PinMAMETimer.Enabled = 1

  '*****************  Nudging  ***********************

  vpmNudge.TiltSwitch  = 1
  vpmNudge.Sensitivity = 5
  vpmNudge.Tiltobj = Array(LeftSlingShot,RightSlingShot,sw45,sw46,sw47) ' Slings and Pop Bumpers

  '************  Trough **************
  Set JPBall1 = sw14.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Set JPBall2 = sw13.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Set JPBall3 = sw12.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Set JPBall4 = sw11.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Set JPBall5 = sw10.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Set JPBall6 = sw9.CreateSizedballWithMass(Ballsize/2,Ballmass)

  Controller.Switch(14) = 1
  Controller.Switch(13) = 1
  Controller.Switch(12) = 1
  Controller.Switch(11) = 1
  Controller.Switch(10) = 1
  Controller.Switch(9) = 1

  '*****************  Captive Ball  ***********************
  Set JPCapBall1 = Captive.CreateSizedballWithMass(Ballsize/2,Ballmass)
  Captive.kick 180,1
  Captive.enabled = false

  'Global ball array (captive balls must go first)
  gBOT = Array(JPCapBall1,JPBall1,JPBall2,JPBall3,JPBall4,JPBall5,JPBall6)

  '*****************  Lights  ***********************
  vpmMapLights InsertLights     ' Map all lamps to the corresponding ROM output using the value of TimerInterval of each light object
  SolGI 0

  '*****************  Other Stuff  ***********************
  Divert 0

  Init_Trex

  LeftSlingShot_Timer
  RightSlingShot_Timer

End Sub

