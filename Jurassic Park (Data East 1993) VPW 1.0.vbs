  BendTime = gametime


  'If Controller.Switch(36) = True then 'if trex is centered  -Is this really needed? (rothbauerw)

'   PlaySound SfxMotorR, 20

    TrexBend()

    If not isEmpty(TrexBall) then
      trexball.x = BM_TRexMain.x - trexballradius*cos(radians(90-RexRot))*Sin(Radians(-trexballanglez - RexBendRot))
      trexball.y = BM_TRexMain.y + trexballradius*sin(radians(90-RexRot))*Sin(Radians(-trexballanglez - RexBendRot))
      trexball.z = BM_TRexMain.z + trexballradius*cos(radians(-trexballanglez - RexBendRot))
      trexball.velx = 0
      trexball.vely = 0
