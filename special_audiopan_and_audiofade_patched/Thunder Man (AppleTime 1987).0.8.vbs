  ElseIf a >= 60 And a < 120 Then
    b = (a - 60) / 60
  ElseIf a >= 120 And a < 240 Then
    b = 1
  ElseIf a >= 240 And a < 300 Then
    b = 1 + (240 - a) / 60
  Else
    b = 0
  End If
  For Each BP in BP_Spinner
    BP.RotX = a
    BP.Opacity = 100 * (1 - b)
  Next
  For Each BP in BP_SpinnerB
    BP.RotX = a
    BP.Opacity = 100 * b
  Next
End Sub


