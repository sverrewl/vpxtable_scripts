Sub SetBackglass()

  For Each VRBGItem In VRBackglass
    VRBGItem.visible = 1
    VRBGItem.x = VRBGItem.x
    VRBGItem.height = - VRBGItem.y + 140
    VRBGItem.y = 0 'adjusts the distance from the backglass towards the user
    VRBGItem.Rotx = -90
  Next

  For Each VRBGItem In VRBackglassDigits
    VRBGItem.x = VRBGItem.x
    VRBGItem.height = - VRBGItem.y + 140
    VRBGItem.y = 0 'adjusts the distance from the backglass towards the user
    VRBGItem.Rotx = -90
  Next
