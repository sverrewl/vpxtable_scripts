Option Explicit : Randomize
Setlocale 1033

'************************************************************************
'                 ZCON: Constants and Global Variables
'************************************************************************

Const BallSize = 50         'Ball diameter in VPX units; must be 50
Const BallMass = 1          'Ball mass must be 1
Const tnob = 7            'Total number of balls the table can hold
Const lob = 3           'Locked balls
Const cGameName = "tf_180"      'The unique alphanumeric name for this table

Dim tablewidth
tablewidth = Table1.width
Dim tableheight
tableheight = Table1.height
Dim BIP               'Balls in play
BIP = 0
Dim BIPL              'Ball in plunger lane
BIPL = False

Dim UseVPMModSol
Dim ModSol      '0 = Non Modulated Lights. 1 = Simple Modulated Lights. 2 = PWM Lights, Requires VPinMame 3.6 or later
Dim PWMflashers

On Error Resume Next
ExecuteGlobal GetTextFile("controller.vbs")
If Err Then MsgBox "You need the controller.vbs in order to run this table, available in the vp10 package"
On Error Goto 0

'Preload lightsettings for UseVPMModSol before VPM loads the script
Dim Loadx
Loadx = LoadValue(cGameName, "MODSOL") : If Loadx <> "" Then ModSol = CInt(Loadx) Else ModSol = 2

If ModSol = 0 Then
  UseVPMModSol = 0
  PWMflashers = 0
End If

If ModSol = 1 Then
  UseVPMModSol = 1
  PWMflashers = 0
End If

If ModSol = 2 Then
  PWMflashers = 1
  UseVPMModSol = 2
End If

'************************************************************************
Const DebugFlashers = True
Const DebugGI = True

Dim bsTrough, bsSaucer, bsKickerMegaTron, plungerIM, cbRight, CarBall, x

Const UseSolenoids = 1
Const UseLamps = 1
Const UseGI = 0
Const UseSync = 0 'set it to 1 if the table runs too fast
Const HandleMech = 0

'----- Desktop Visible Elements -----
Dim VarHidden, UseVPMDMD

If Table1.ShowDT = true and RenderingMode <> 2 Then
  UseVPMDMD = True
  VarHidden = 1
    PinCab_Rails.Visible = 1
Else
  UseVPMDMD = False
  VarHidden = 0
    PinCab_Rails.Visible = 0
End If

'----- VR Room Auto-Detect -----
Dim VRRoom, VR_Obj, VRMode
Const VRTest = 0        ' 1 = Testing VR in Live View, 0 = Do not force VR mode.

If RenderingMode = 2 or Table1.ShowFSS = True or VRTest = 1 Then
  Textbox1.visible = 0
  For Each VR_Obj in VRCabinet : VR_Obj.Visible = 1 : Next
  For Each VR_Obj in VRMinimalRoom : VR_Obj.Visible = 1 : Next
  UseVPMDMD = True
  VarHidden = 0
  VRMode = True
    PinCab_Rails.Visible = 1
  If VRRoomChoice = 1 Then
    For Each VR_Obj in VRMinimalRoom : VR_Obj.Visible = 0 : Next
    For Each VR_Obj in VRMegaRoom : VR_Obj.Visible = 1 : Next
  Else
    For Each VR_Obj in VRMinimalRoom : VR_Obj.Visible = 1 : Next
    For Each VR_Obj in VRMegaRoom : VR_Obj.Visible = 0 : Next
  End If
Else
  VRMode = False
  For Each VR_Obj in VRCabinet : VR_Obj.Visible = 0 : Next
  For Each VR_Obj in VRMinimalRoom : VR_Obj.Visible = 0 : Next
  For Each VR_Obj in VRMegaRoom : VR_Obj.Visible = 0 : Next
End If

if B2SOn = True then VarHidden = 1

LoadVPM "01550000", "SAM.VBS", 3.26

' Standard Sounds
Const SSolenoidOn = ""
Const SSolenoidOff = ""
Const SCoin = ""

'*******************************************
' ZTIM:Timers
'*******************************************

Dim FrameTime, InitFrameTime
InitFrameTime = 0
Sub FrameTimer_Timer() 'The frame timer interval should be -1, so executes at the display frame rate
  FrameTime = GameTime - InitFrameTime
  InitFrameTime = GameTime  'Count frametime
  RollingUpdate   'update rolling sounds
  DoSTAnim          'Standup target animations
  Options_UpdateDMD
  If DynamicBallShadowsOn Or AmbientBallShadowOn Then DynamicBSUpdate 'update ball shadows
  If li1.state > 0.05 Then
    Pincab_StartButton2.disablelighting = 0
  Else
    Pincab_StartButton2.disablelighting = 1
  End if
  If li2.state > 0.05 Then
    Pincab_TourneyButton.disablelighting = 0
  Else
    Pincab_TourneyButton.disablelighting = 1
  End if
End Sub

'The CorTimer interval should be 10. It's sole purpose is to update the Cor calculations
CorTimer.Interval = 10
Sub CorTimer_Timer(): Cor.Update: End Sub

'************
' Table init.
'************

Sub Table1_Init
  vpmInit me
  InitVpmFFlipsSAM
  With Controller
        .GameName = cGameName
        If Err Then MsgBox "Can't start Game" & cGameName & vbNewLine & Err.Description:Exit Sub
