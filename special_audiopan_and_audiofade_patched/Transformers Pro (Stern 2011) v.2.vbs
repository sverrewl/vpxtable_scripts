Sub Table1_KeyDown(ByVal Keycode)
  If keycode = LeftFlipperKey Then
    PinCab_LeftFlipperButton.X = PinCab_LeftFlipperButton.X + 6
  End If
  If keycode = RightFlipperKey Then
    PinCab_RightFlipperButton.X = PinCab_RightFlipperButton.X - 6
  End If
  If keycode = LeftTiltKey Then
    Nudge 90, 1
    SoundNudgeLeft
  End If
  If keycode = RightTiltKey Then
    Nudge 270, 1
    SoundNudgeRight
  End If
  If keycode = CenterTiltKey Then
    Nudge 0, 1
    SoundNudgeCenter
  End If
  If keycode = PlungerKey Then
    Plunger.Pullback
    SoundPlungerPull
    TimerPlunger.Enabled = True
    TimerPlunger2.Enabled = False
  End If
  If keycode = keyInsertCoin1 or keycode = keyInsertCoin2 or keycode = keyInsertCoin3 or keycode = keyInsertCoin4 Then
    Select Case Int(rnd*3)
      Case 0: PlaySound ("Coin_In_1"), 0, CoinSoundLevel, 0, 0.25
      Case 1: PlaySound ("Coin_In_2"), 0, CoinSoundLevel, 0, 0.25
      Case 2: PlaySound ("Coin_In_3"), 0, CoinSoundLevel, 0, 0.25
    End Select
  End If
  If keycode = StartGameKey Then
    Controller.Switch(16)  = True
        soundStartButton()
    Pincab_StartButton.Y = Pincab_StartButton.Y -5
    Pincab_StartButton2.Y = Pincab_StartButton2.Y -5
  End If
  If keycode = keyFront Then
    Controller.Switch(15) = True
    soundStartButton()
    Pincab_TourneyButton.Y =Pincab_TourneyButton.Y -5 'Tournament mode mapped to key 2
  End If
  If bInOptions Then
    Options_KeyDown keycode
    Exit Sub
  End If
  If keycode = LeftMagnaSave Then
    If bOptionsMagna Then Options_Open() Else bOptionsMagna = True
    ElseIf keycode = RightMagnaSave Then
    If bOptionsMagna Then Options_Open() Else bOptionsMagna = True
  End If
    If vpmKeyDown(keycode) Then Exit Sub
End Sub

Sub Table1_KeyUp(ByVal Keycode)
  If keycode = LeftFlipperKey Then
    PinCab_LeftFlipperButton.X = PinCab_LeftFlipperButton.X - 6
  End If
  If keycode = RightFlipperKey Then
    PinCab_RightFlipperButton.X = PinCab_RightFlipperButton.X + 6
  End If
  If KeyCode = PlungerKey Then
    Plunger.Fire
    TimerPlunger.Enabled = False
    TimerPlunger2.Enabled = True
    PinCab_Plunger.Y = 2360
    If BIPL = 1 Then
      SoundPlungerReleaseBall()   'Plunger release sound when there is a ball in shooter lane
    Else
      SoundPlungerReleaseNoBall() 'Plunger release sound when there is no ball in shooter lane
    End If
  End If
  If keycode = StartGameKey Then
    Controller.Switch(16)  = False
    Pincab_StartButton.Y = Pincab_StartButton.Y +5
    Pincab_StartButton2.Y = Pincab_StartButton2.Y +5
  End if
  If keycode = keyFront Then
    Controller.Switch(15) = False
    Pincab_TourneyButton.Y =Pincab_TourneyButton.Y +5 'Tournament mode mapped to key 2
  End if
  If keycode = LeftMagnaSave And Not bInOptions Then bOptionsMagna = False
    If keycode = RightMagnaSave And Not bInOptions Then bOptionsMagna = False
  If vpmKeyUp(keycode) Then Exit Sub
End Sub

