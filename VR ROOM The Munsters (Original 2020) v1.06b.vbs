  Function Balls
    Dim tmp

    tmp = bpgcurrent - BallsRemaining(CurrentPlayer) + 1
    If tmp> bpgcurrent Then
      Balls = bpgcurrent
    Else
      Balls = tmp
    End If
  End Function

  Sub FirstBall
