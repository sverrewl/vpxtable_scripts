              lStates(CurrentPlayer,25)=1:L25.state=1 ' shoot again
            ElseIf lStates(CurrentPlayer,64) <> 0 Then 'MMadness
              ScoopDelay=22500
              StartMMadnessMode()
            Else 'Score random value
              ScoopDelay=1000
              ScoreScoop()
            End If
          End If
        End If
      End If
    End If
