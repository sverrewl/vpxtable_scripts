    End If
    BonusMultiplier(CurrentPlayer)=BonusMultiplier(CurrentPlayer)+b
    Page5VideoClip 3000, "", tline, "", "videospecials", "dragula.mp4"
    PaintModes()
    vpmtimer.addtimer 3000, "ScoreJackPots '"  '
