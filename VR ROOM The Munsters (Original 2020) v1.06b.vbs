      Case 3:LSLing1.Visible = 0:LSLing2.Visible = 1:sling2.TransZ = -10
      Case 4:LSLing2.Visible = 0:LSLing.Visible = 1:sling2.TransZ = 0:LeftSlingShot.TimerEnabled = 0
