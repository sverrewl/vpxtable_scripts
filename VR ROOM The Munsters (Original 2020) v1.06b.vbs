    case 3
      a=spot
      If Boost(CurrentPlayer,a)=1 Then
        a=lily
        If Boost(CurrentPlayer,a)=1 Then
          a=herman
          If Boost(CurrentPlayer,a)=1 Then
            a=Grandpa
            If Boost(CurrentPlayer,a)=1 Then
              a=0
