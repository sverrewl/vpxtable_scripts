    If rndIntro=2 Then UltraDMD.DisplayScene00Ex "CLIntro2.wmv", "", -1, -1,"", -1, -1, UltraDMD_Animation_FadeIn, introtime, UltraDMD_Animation_FadeOut
    DisplayIntroHS.enabled=1
