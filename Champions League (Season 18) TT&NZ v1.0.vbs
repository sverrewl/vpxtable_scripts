Case 5    'x.xxx
FormatScore="         " & formatnumber(scores(CurrentPlayer),0)
Case 6    'xx.xxx
FormatScore="        " & formatnumber(scores(CurrentPlayer),0)
Case 7    'xxx.xxx
FormatScore="       " & formatnumber(scores(CurrentPlayer),0)
Case 9    'x.xxx.xxx
FormatScore="     " & formatnumber(scores(CurrentPlayer),0)
Case 10   'xx.xxx.xxx
FormatScore="    " & formatnumber(scores(CurrentPlayer),0)
Case 11   'xxx.xxx.xxx
