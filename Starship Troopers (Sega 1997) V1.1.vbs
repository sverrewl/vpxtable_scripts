Sub SolWarrior(Enabled)
  If Enabled Then
    BugLit=True
    BugWalls(Position).IsDropped=1
    BugTargets(Position).IsDropped=1
    Position=Position+6
    BugWalls(Position).IsDropped=0
    BugTargets(Position).IsDropped=0
