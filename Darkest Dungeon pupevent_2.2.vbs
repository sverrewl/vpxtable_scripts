          End If
        Case 14:
          if AMState = 1 Then
            BallHandlingQueue.Add "InnKickOut","InnKickBallOut",70,KickOutDelay,0,0,0,True
          End If
        Case Else:
          'Do Nothing
          BallHandlingQueue.Add "InnKickOut","InnKickBallOut",70,KickOutDelay,0,0,0,True
      End Select
  Else ' in one of the side missions kick ball out
