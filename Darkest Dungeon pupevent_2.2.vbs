    GeneralPuPQueue.Add "Pup899","PuPEvent 899",99,0,0,0,0,True' Tilt Video
    GeneralPuPQueue.Add "EOBReturn-Tilt","EOBReturn ""CheckTilt""",91,7000,0,0,0,False
