      debug3 "Leaving with: "&col
      debug3 "================================="
      Exit do
    End If
  m = m + 1
Loop

if m = MaxPasses Then
  ' force selection via brute force
  rndArrayCol = ForceSelection(arrayIn)
  Debug3 "Force Response: " &rndArrayCol
  debug3 "---------------------------------"
Else
  rndArrayCol = col ' return value from while loop
