Sub enableLaneLights(bEnable)
  dim light
  bSSLaneLightsEnabled=bEnable
  if bSSLaneLightsEnabled then
    ChangeGi GILane, "white", 1
    For Each light In GILane
      SetSlowPulse3(light)
      light.state=2
    Next
  else
    For Each light In GILane
      SetDefPulse3(light)
      light.state=1
    Next
  End if
