    End if
    DOF 150, DOFPulse 'shaker, fan, mx
    QueueSceneS2 sSwapPos, "bBatTurnProgressActive=False ", -1, 1, True
    QueueSceneS sSwapPos, "StartLastSceneScreen sSwapPos ", -1, 1

  elseif bBatTurnProgressActive then
