Sub EnableBallSaver(seconds)
'WriteToLog "     ", "Ballsaver started " & seconds
    ' set our game flag
    bBallSaverActive = True
    bBallSaverReady = False
    ' start the timer
  BallSaverTimerExpired.Interval = 10
