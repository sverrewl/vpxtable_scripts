      if Not (PlayerState(CurrentPlayer).isActive(kMinV_S2Shame) and PlayerState(CurrentPlayer).TProgress(kMinV_S2Shame)<=4 and LightL.State=2) then  ' If we are in Shame and this is blinking skip it
