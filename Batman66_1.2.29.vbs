Sub BatPoleSeqFinish()
  dim titleStr
  dim tmpStr
  Dim curScore
  Dim bMute:bMute=0
  dim bHas3:bHas3=True
  dim vidLen2:vidLen2=1330
  dim vidLen3:vidLen3=3000
