  If FlashLevel5 < 0 Then
    Flasherflash5.TimerEnabled = False
    Flasherflash5_5.TimerEnabled = False
    Flasherflash5.visible = 0
    Flasherflash5_5.visible = 0
    DOF 114,0:DOF 115,0:DOF 116,0
