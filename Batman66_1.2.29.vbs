        rndIdx2= INT(Rnd*6)
        while rndIdx2 = rndIdx
          rndIdx2=Int(Rnd*6)
        wend

        rndIdx3= INT(Rnd*6)
        while rndIdx3 = rndIdx2 or rndIdx3 = rndIdx
          rndIdx3=Int(Rnd*6)
        wend

