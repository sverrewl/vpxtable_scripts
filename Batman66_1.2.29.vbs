  End if

  if PlayersPlayingGame<>1 then                   ' This only applies to Multiplayer
    if bBackGateIsOpen=False then exit sub            ' ERROR - Ball cant hit then when back gate is closed
    vpmTimer.AddTimer 1000, "HandleLockTrough False '"
  End if
End Sub

