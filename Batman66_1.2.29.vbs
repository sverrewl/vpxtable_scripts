    LogFileObj.WriteToLog label, message
  end if
End Sub

Sub NewLog()
  if KeepLogs Then
    LogInit=True
    Set LogFileObj = New DebugLogFile
    LogFileObj.LogInit
    LogFileObj.WriteToLog "NEW LOG", " "
  end if
End Sub




