          ' Light Joker for final before mode start
          if PlayerState(CurrentPlayer).TProgress(kWiz_BatTurn)=18 then
            bFlash1Enabled=True
            DOF 128, DOFOn  'Beacon On
          elseif PlayerState(CurrentPlayer).TProgress(kWiz_BatTurn)=20 then
