        PlayerState(CurrentPlayer).SSetLightColor kWiz_Holy, kLightJokerArrow, "blue", 2
        TurnTable_Goto(kTT_Batcave)
        bFlash1Enabled=True
        DOF 128, DOFOn  'Beacon On
