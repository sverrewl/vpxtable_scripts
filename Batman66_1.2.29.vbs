  if DoDebounce(kWiz_Batusi, light.name) then WriteToLog "     ", "Process Batusi Debounce Skip":exit sub

'WriteToLog "     ", "Process Batusi " & light.name & " Progress:" & PlayerState(CurrentPlayer).TProgress(kWiz_Batusi)
  Dim bValidHit
