  elseif bInMultiplayerSelect and bAcceptIdx<>-1 then
    If keycode = LeftFlipperKey Then
      bAcceptIdx=1
      puPlayer.LabelSet pOverVid,"PlayerList3", "[YES]  NO" ,1,"{'mt':2,'color':" & pupOrange & ", 'yalign':0,'xalign':1, 'size':9}"

