    Do
      cnt=cnt+1
      PlayerMode = (PlayerMode - 1)
      if (PlayerMode < 0) then PlayerMode=7
    Loop While ModePercent(PlayerMode) >= 100 and cnt<8
        UpdatePlayerMode
    End If
