  End if

  if (bDebugLeftMagnaDown and bDebugRightMagnaDown = False) then
    vpmtimer.addtimer 200, "HandleDebugLeft '"
  Elseif (bDebugLeftMagnaDown = False and bDebugRightMagnaDown) then
    vpmtimer.addtimer 200, "HandleDebugRight '"
