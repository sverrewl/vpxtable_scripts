Sub SelectPlayerMode(keycode)
  Dim cnt
  cnt = 0

    If keycode = LeftFlipperKey Then
        PlaySoundVol "fx_Previous", VolDef
    Do
      cnt=cnt+1
      PlayerMode = (PlayerMode + 1)MOD 8
    Loop While ModePercent(PlayerMode) >= 100 and cnt<8
        UpdatePlayerMode
    End If
    If keycode = RightFlipperKey Then
